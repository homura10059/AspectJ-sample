<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO" xmlns="http://logging.apache.org/log4j/2.0/config">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy/MM/dd HH:mm:ss.SSS} [%t]%-5level %C %M %L %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="Default" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>